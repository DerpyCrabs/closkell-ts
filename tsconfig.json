{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "types": [
            "bun-types",
            "node"
        ],
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "allowImportingTsExtensions": true,
        "noEmit": true
    }
}